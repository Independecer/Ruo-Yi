            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where referral_id = #{referralId}
    </update>

    <delete id="deletePsyConsultantReferralOrderByReferralId" parameterType="Long">
        delete from psy_consultant_referral_order where referral_id = #{referralId}
    </delete>

    <delete id="deletePsyConsultantReferralOrderByReferralIds" parameterType="String">
        delete from psy_consultant_referral_order where referral_id in 
        <foreach item="referralId" collection="array" open="(" separator="," close=")">
            #{referralId}
        </foreach>
    </delete>
</mapper>