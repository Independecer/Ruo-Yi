    <sql id="selectPsyConsultPartnerVo">
        select id, user_id, c_id, step, type, name, sex, wechat, qq, phone, email, city, province, lang, work_hours, genre, ext_genre, card, card_img, extra_img, status, pay, create_by, create_time, update_by, update_time from psy_consult_partner
    </sql>

    <select id="selectPsyConsultPartnerList" parameterType="PsyConsultPartner" resultMap="PsyConsultPartnerResult">
        <include refid="selectPsyConsultPartnerVo"/>
        <where>  
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="cId != null "> and c_id = #{cId}</if>
            <if test="step != null  and step != ''"> and step = #{step}</if>
            <if test="type != null  and type != ''"> and type = #{type}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="sex != null  and sex != ''"> and sex = #{sex}</if>
            <if test="wechat != null  and wechat != ''"> and wechat = #{wechat}</if>
            <if test="qq != null  and qq != ''"> and qq = #{qq}</if>
            <if test="phone != null  and phone != ''"> and phone = #{phone}</if>
            <if test="email != null  and email != ''"> and email = #{email}</if>
            <if test="city != null  and city != ''"> and city = #{city}</if>
            <if test="province != null  and province != ''"> and province = #{province}</if>
            <if test="lang != null  and lang != ''"> and lang = #{lang}</if>
            <if test="workHours != null  and workHours != ''"> and work_hours = #{workHours}</if>
            <if test="genre != null  and genre != ''"> and genre = #{genre}</if>
            <if test="card != null  and card != ''"> and card = #{card}</if>
            <if test="cardImg != null  and cardImg != ''"> and card_img = #{cardImg}</if>
            <if test="extraImg != null  and extraImg != ''"> and extra_img = #{extraImg}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="pay != null  and pay != ''"> and pay = #{pay}</if>
        </where>
    </select>

    <insert id="insertPsyConsultPartner" parameterType="PsyConsultPartner">
        insert into psy_consult_partner
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
