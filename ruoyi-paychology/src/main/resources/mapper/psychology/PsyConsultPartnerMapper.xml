            <if test="cId != null">c_id,</if>
            <if test="step != null">step,</if>
            <if test="type != null">type,</if>
            <if test="name != null and name != ''">name,</if>
            <if test="sex != null">sex,</if>
            <if test="wechat != null">wechat,</if>
            <if test="qq != null">qq,</if>
            <if test="phone != null">phone,</if>
            <if test="email != null">email,</if>
            <if test="city != null">city,</if>
            <if test="province != null">province,</if>
            <if test="lang != null">lang,</if>
            <if test="workHours != null and workHours != ''">work_hours,</if>
            <if test="genre != null">genre,</if>
            <if test="card != null">card,</if>
            <if test="cardImg != null">card_img,</if>
            <if test="extraImg != null">extra_img,</if>
            <if test="status != null">status,</if>
            <if test="pay != null">pay,</if>
