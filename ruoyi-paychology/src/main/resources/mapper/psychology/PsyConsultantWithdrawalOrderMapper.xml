<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.renxin.psychology.mapper.PsyConsultantWithdrawalOrderMapper">
    
    <resultMap type="PsyConsultantWithdrawalOrder" id="PsyConsultantWithdrawalOrderResult">
        <result property="withdrawalNo"    column="withdrawal_no"    />
        <result property="consultantId"    column="consultant_id"    />
        <result property="status"    column="status"    />
        <result property="withdrawalAmount"    column="withdrawal_amount"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectPsyConsultantWithdrawalOrderVo">
        select withdrawal_no, consultant_id, status, withdrawal_amount, del_flag, create_by, create_time, update_by, update_time from psy_consultant_withdrawal_order
    </sql>

    <select id="selectPsyConsultantWithdrawalOrderList" parameterType="PsyConsultantWithdrawalOrder" resultMap="PsyConsultantWithdrawalOrderResult">
        <include refid="selectPsyConsultantWithdrawalOrderVo"/>
        <where>  
            <if test="consultantId != null "> and consultant_id = #{consultantId}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="withdrawalAmount != null "> and withdrawal_amount = #{withdrawalAmount}</if>
        </where>
    </select>
    
    <select id="selectPsyConsultantWithdrawalOrderByWithdrawalNo" parameterType="Long" resultMap="PsyConsultantWithdrawalOrderResult">
        <include refid="selectPsyConsultantWithdrawalOrderVo"/>
        where withdrawal_no = #{withdrawalNo}
    </select>
        
    <insert id="insertPsyConsultantWithdrawalOrder" parameterType="PsyConsultantWithdrawalOrder">
        insert into psy_consultant_withdrawal_order
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="withdrawalNo != null">withdrawal_no,</if>
            <if test="consultantId != null">consultant_id,</if>
            <if test="status != null">status,</if>
            <if test="withdrawalAmount != null">withdrawal_amount,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="withdrawalNo != null">#{withdrawalNo},</if>
            <if test="consultantId != null">#{consultantId},</if>
            <if test="status != null">#{status},</if>
            <if test="withdrawalAmount != null">#{withdrawalAmount},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updatePsyConsultantWithdrawalOrder" parameterType="PsyConsultantWithdrawalOrder">
        update psy_consultant_withdrawal_order
        <trim prefix="SET" suffixOverrides=",">
            <if test="consultantId != null">consultant_id = #{consultantId},</if>
            <if test="status != null">status = #{status},</if>
            <if test="withdrawalAmount != null">withdrawal_amount = #{withdrawalAmount},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where withdrawal_no = #{withdrawalNo}
    </update>

    <delete id="deletePsyConsultantWithdrawalOrderByWithdrawalNo" parameterType="Long">
        delete from psy_consultant_withdrawal_order where withdrawal_no = #{withdrawalNo}
    </delete>

    <delete id="deletePsyConsultantWithdrawalOrderByWithdrawalNos" parameterType="String">
        delete from psy_consultant_withdrawal_order where withdrawal_no in 
        <foreach item="withdrawalNo" collection="array" open="(" separator="," close=")">
            #{withdrawalNo}
        </foreach>
    </delete>
</mapper>