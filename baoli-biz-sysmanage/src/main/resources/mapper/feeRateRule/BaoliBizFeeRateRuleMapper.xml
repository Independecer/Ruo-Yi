<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baoli.store.mapper.BaoliBizFeeRateRuleMapper">
    
    <resultMap type="BaoliBizFeeRateRule" id="BaoliBizFeeRateRuleResult">
        <result property="id"    column="id"    />
        <result property="title"    column="title"    />
        <result property="feeRateType"    column="fee_rate_type"    />
        <result property="bankId"    column="bank_id"    />
        <result property="bankName"    column="bank_name"    />
        <result property="remark"    column="remark"    />
        <result property="brandId"    column="brand_id"    />
        <result property="brandName"    column="brand_name"    />
        <result property="level2BrandName"    column="level2_brand_name"    />
        <result property="level2BrandId"    column="level2_brand_id"    />
        <result property="modelId"    column="model_id"    />
        <result property="seriesName"    column="series_name"    />
        <result property="seriesId"    column="series_id"    />
        <result property="modelName"    column="model_name"    />
        <result property="modelId"    column="model_id"    />
        <result property="subsidyType"    column="subsidy_type"    />
        <!--result property="content"    column="content"    /-->
        <result property="period"    column="period"    />
        <result property="totalFeeRate"    column="total_fee_rate"    />
        <result property="rebateRate"    column="rebate_rate"    />
        <result property="cardPersonRate"    column="card_person_rate"    />
        <result property="storeFeeRate"    column="store_fee_rate"    />
        <result property="startTime"    column="start_time"    />
        <result property="finishTime"    column="finish_time"    />
    </resultMap>

    <sql id="selectBaoliBizFeeRateRuleVo">
        select a.start_time,a.finish_time,a.series_id,a.period,a.total_fee_rate,a.rebate_rate,a.card_person_rate,a.store_fee_rate,(select b.`name` from  baoli_biz_car_brand b where b.id = a.level2_brand_id) as level2_brand_name,(select b.`name` from  baoli_biz_car_brand b where b.id = a.brand_id) as brand_name,(select b.name from baoli_biz_car_series b where b.id = a.series_id) series_name,(select b.name from baoli_biz_car_model b where b.id = a.model_id) model_name,id, title, fee_rate_type,(SELECT dict_label FROM sys_dict_data b where dict_type ='bl_biz_bank' and b.dict_value = a.bank_id) bank_name, bank_id, remark, brand_id,level2_brand_id, model_id, subsidy_type from baoli_biz_fee_rate_rule a
    </sql>

    <select id="selectBaoliBizFeeRateRuleList" parameterType="BaoliBizFeeRateRule" resultMap="BaoliBizFeeRateRuleResult">
        <include refid="selectBaoliBizFeeRateRuleVo"/>
        <where>  
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="feeRateType != null  and feeRateType != ''"> and fee_rate_type = #{feeRateType}</if>
            <if test="bankId != null "> and bank_id = #{bankId}</if>
            <if test="brandId != null "> and brand_id = #{brandId}</if>
            <if test="level2BrandId != null "> and level2_brand_id = #{level2BrandId}</if>
            <if test="seriesId != null "> and series_id = #{seriesId}</if>
            <if test="modelId != null "> and model_id = #{modelId}</if>
            <if test="subsidyType != null  and subsidyType != ''"> and subsidy_type = #{subsidyType}</if>
        </where>
    </select>
    
    <select id="selectBaoliBizFeeRateRuleById" parameterType="Long" resultMap="BaoliBizFeeRateRuleResult">
        <include refid="selectBaoliBizFeeRateRuleVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBaoliBizFeeRateRule" parameterType="BaoliBizFeeRateRule" useGeneratedKeys="true" keyProperty="id">
        insert into baoli_biz_fee_rate_rule
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="title != null">title,</if>
            <if test="feeRateType != null">fee_rate_type,</if>
            <if test="bankId != null">bank_id,</if>
            <if test="remark != null">remark,</if>
            <if test="brandId != null">brand_id,</if>
            <if test="level2BrandId != null">level2_brand_id,</if>
            <if test="seriesId != null">series_id,</if>
            <if test="modelId != null">model_id,</if>
            <if test="subsidyType != null">subsidy_type,</if>
            <if test="period != null">period,</if>
            <if test="totalFeeRate != null">total_fee_rate,</if>
            <if test="rebateRate != null">rebate_rate,</if>
            <if test="cardPersonRate != null">card_person_rate,</if>
            <if test="storeFeeRate != null">store_fee_rate,</if>
            <if test="startTime != null">start_time,</if>
            <if test="finishTime != null">finish_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="title != null">#{title},</if>
            <if test="feeRateType != null">#{feeRateType},</if>
            <if test="bankId != null">#{bankId},</if>
            <if test="remark != null">#{remark},</if>
            <if test="brandId != null">#{brandId},</if>
            <if test="level2BrandId != null">#{level2BrandId},</if>
            <if test="seriesId != null">#{seriesId},</if>
            <if test="modelId != null">#{modelId},</if>
            <if test="subsidyType != null">#{subsidyType},</if>
            <if test="period != null">#{period},</if>
            <if test="totalFeeRate != null">#{totalFeeRate},</if>
            <if test="rebateRate != null">#{rebateRate},</if>
            <if test="cardPersonRate != null">#{cardPersonRate},</if>
            <if test="storeFeeRate != null">#{storeFeeRate},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="finishTime != null">#{finishTime},</if>
         </trim>
    </insert>

    <update id="updateBaoliBizFeeRateRule" parameterType="BaoliBizFeeRateRule">
        update baoli_biz_fee_rate_rule
        <trim prefix="SET" suffixOverrides=",">
            <if test="title != null">title = #{title},</if>
            <if test="feeRateType != null">fee_rate_type = #{feeRateType},</if>
            <if test="bankId != null">bank_id = #{bankId},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="brandId != null">brand_id = #{brandId},</if>
            <if test="level2BrandId != null">level2_brand_id = #{level2BrandId},</if>
            <if test="seriesId != null">series_id = #{seriesId},</if>
            <if test="modelId != null">model_id = #{modelId},</if>
            <if test="subsidyType != null">subsidy_type = #{subsidyType},</if>
            <if test="period != null">period = #{period},</if>
            <if test="totalFeeRate != null">total_fee_rate = #{totalFeeRate},</if>
            <if test="rebateRate != null">rebate_rate = #{rebateRate},</if>
            <if test="cardPersonRate != null">card_person_rate = #{cardPersonRate},</if>
            <if test="storeFeeRate != null">store_fee_rate = #{storeFeeRate},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="finishTime != null">finish_time = #{finishTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBaoliBizFeeRateRuleById" parameterType="Long">
        delete from baoli_biz_fee_rate_rule where id = #{id}
    </delete>

    <delete id="deleteBaoliBizFeeRateRuleByIds" parameterType="String">
        delete from baoli_biz_fee_rate_rule where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>